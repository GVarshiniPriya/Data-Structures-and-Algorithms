#include<iostream>
using namespace std;
int main() {
 int i, n, j, k = 0;
 int a[10], b[10];
 cout << "Enter the number of elements of the array: " << endl;
  cin >> n;
  cout << "Enter the elements of the array: " << endl;
   for (i = 0; i < n; i++) 
        {cin >> a[i]; }
    for (i = 0; i < n; i++)
    {
      for (j = 0; j < k; j++) {
          if (a[i] == b[j]) 
              break;   }
       if (j == k)
          { b[k] = a[i];
            k++; }
   }
  cout << "The array after removal of repeated elements : " <<endl;
    for (i = 0; i < k; i++)
       { cout << b[i] << " ";} 
return 0;
}
