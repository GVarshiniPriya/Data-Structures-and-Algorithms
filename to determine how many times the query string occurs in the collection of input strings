#include<iostream>
#include<cstring>
using namespace std;
int main() 
{
char arr1[5][20];char arr2[4][20];
  cout << "Enter only 5 elements for the array 1 :" << endl;
 for(int i = 0; i < 5; i++) 
      {cin >> arr1[i];}
cout << "Enter only 4 elements for the array 2:" << endl;
for(int i = 0; i < 4; i++)  
      { cin >> arr2[i];}
for(int i = 0; i < 4; i++) 
{int c = 0;
    for(int j = 0; j < 5; j++) 
         {  if(strcmp(arr2[i], arr1[j]) == 0) 
                c++;   }
cout << c << " "; } 
return 0;
}
